{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.inctue.customwebportal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "579712a2-f66e-4b0c-8e32-04f9bb952393"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.inctue.customwebportal.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "idMainApp",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.inctue.customwebportal.view",
        "async": true,
        "viewPath": "com.inctue.customwebportal.view",
        "clearControlAggregation": true
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "",
          "target": [
            "TargetApp"
          ]
        },
        {
          "name": "RouteAnnouncementDetails",
          "pattern": "announcements",
          "target": [
            "TargetAnnouncementDetails"
          ]
        },
        {
          "name": "RouteKnowledgeArticleDetails",
          "pattern": "knowledge-articles",
          "target": [
            "TargetKnowledgeArticleDetails"
          ]
        }
      ],
      "targets": {
        "TargetApp": {
          "id": "AppMain",
          "name": "App"
        },
        "TargetAnnouncementDetails": {
          "id": "AnnouncementDetails",
          "name": "AnnouncementDetails"
        },
        "TargetKnowledgeArticleDetails": {
          "id": "KnowledgeArticleDetails",
          "name": "KnowledgeArticleDetails"
        }
      }
    },
    "rootView": {
      "viewName": "com.inctue.customwebportal.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}